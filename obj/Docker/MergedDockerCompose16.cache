services:
  adminer:
    image: adminer
    ports:
    - published: 8080
      target: 8080
    restart: always
  beamerblazor:
    build:
      context: C:\Users\natma\source\repos\PIM
      dockerfile: BeamerBlazor/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BeamerBlazor
    container_name: BeamerBlazor
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: beamerblazor
    labels:
      com.microsoft.visual-studio.project-name: BeamerBlazor
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/BeamerBlazor.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\natma\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\natma\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\natma\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  db:
    environment:
      POSTGRES_PASSWORD: Mn@205133
